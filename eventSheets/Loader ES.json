					"children": [
						{
							"eventType": "comment",
							"text": "We go to a level if its value does not exceed the total number of levels."
						},
						{
							"eventType": "block",
							"conditions": [
								{
									"id": "compare-eventvar",
									"objectClass": "System",
									"sid": 708230273447613,
									"parameters": {
										"variable": "Level",
										"comparison": 3,
										"value": "AllLevelsCount"
									}
								}
							],
							"actions": [
								{
									"type": "script",
									"script": "if (window.platforms == 'YA')\n{\nwindow.ysdk.features.LoadingAPI.ready();\n}\nif (window.bestscore>runtime.globalVars.Level)\n{\nruntime.globalVars.Level=window.bestscore;\n}"
								},
								{
									"id": "go-to-layout-by-name",
									"objectClass": "System",
									"sid": 353698222943437,
									"parameters": {
										"layout": "\"Lvl-\" & Level"
									}
								}
							],
							"sid": 826922659968308
						},
						{
							"eventType": "comment",
							"text": "Otherwise, we go to the random level."
						},
						{
							"eventType": "block",
							"conditions": [
								{
									"id": "compare-eventvar",
									"objectClass": "System",
									"sid": 599867552055190,
									"parameters": {
										"variable": "Level",
										"comparison": 4,
										"value": "AllLevelsCount"
									}
								}
							],
							"actions": [
								{
									"type": "script",
									"script": "if (window.platforms == 'YA')\n{\nwindow.ysdk.features.LoadingAPI.ready();\n}\nif (window.bestscore>runtime.globalVars.Level)\n{\nruntime.globalVars.Level=window.bestscore;\n}"
								},
								{
									"id": "go-to-layout-by-name",
									"objectClass": "System",
									"sid": 884982204355926,
									"parameters": {
										"layout": "\"Lvl-\" & int(random(2, AllLevelsCount))"
									}
								}
							],
							"sid": 513874100530781
						}
					]
				}
			]
		},
		{
			"eventType": "script",
			"script": "if (window.platforms == 'YA')\n{\nruntime.globalVars.sdkloaded = window.sdkloaded;\nruntime.globalVars.adsopened = window.adsopened;\nruntime.globalVars.testnumber = window.testnumber;\n}\nif (window.platforms == 'VK')\n{\nruntime.globalVars.sdkloaded = window.sdkloaded;\nruntime.globalVars.adsopened = window.adsopened;\nruntime.globalVars.lang = 0;\n}\nif (window.platforms == 'OK')\n{\nruntime.globalVars.sdkloaded = window.sdkloaded;\nruntime.globalVars.adsloaded = window.adsloaded;\nruntime.globalVars.adsopened = window.adsopened;\nruntime.globalVars.rewardcomplete = window.rewardcomplete;\n\n}"
